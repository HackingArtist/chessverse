{
    "name":"ChessDapp",
    "version":"0.0.1",
    "manifest_version":2,

    "browser_action":{
        "default_popup":"index.html"
    },

      "permissions": ["activeTab","tabs", "*://*.chess.com/*"],
      "content_scripts": [
        {
           
          "matches": ["*://*.chess.com/*"],
          "js": ["popup.js"]
        }
      ]
}
